

generator client {
  provider = "prisma-client"
  
}

datasource db {
  provider = "postgresql"
}


model users{ 
  id   Int  @default(autoincrement()) @id
  username  String 
  password String
  age Int
  city String

}

model todos{
  id             Int  @default(autoincrement()) @id
  title          String
  description    String
  done           Boolean
  userid         Int


}